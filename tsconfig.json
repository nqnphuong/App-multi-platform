{
  "extends": "@tsconfig/react-native/tsconfig.json",
   "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@api/*": ["api/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@models/*": ["models/*"],
      "@navigation/*": ["navigation/*"],
      "@store/*": ["store/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
      "@screens/*": ["screens/*"]
    }
  }
}
